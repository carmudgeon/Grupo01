<?xml version="1.0" encoding="UTF-8" standalone="no"?>
	<featureModel chosenLayoutAlgorithm="1">
		<struct>
			<and abstract="true" mandatory="true" name="Inmobiliaria">
				<and mandatory="true" name="GestionDeActivos">
					<feature name="RevisionesMercado"/>
					<or mandatory="true" name="Reporting">
						<feature mandatory="true" name="Operativo"/>
						<feature mandatory="true" name="Financiero"/>
					</or>
					<feature name="Refinanciacion"/>
					<feature mandatory="true" name="AdquisicionInmuebles"/>
					<feature name="Contabilidad"/>
					<feature mandatory="true" name="Alquileres"/>
					<feature name="Auditorias"/>
					<feature name="VentaActivos"/>
					<feature name="Valoraciones"/>
				</and>
				<and name="GestionDeCapital">
					<or mandatory="true" name="Inversiones">
						<feature mandatory="true" name="Alternativas"/>
						<feature mandatory="true" name="Tradicionales"/>
					</or>
					<feature mandatory="true" name="AdquisicionYVentaDeActivos"/>
					<feature name="RelacionInversores"/>
					<feature name="DefinicionGobiernoCorporativo"/>
					<and name="TipoTransferencia">
						<or mandatory="true" name="Seguros">
							<feature mandatory="true" name="PatimoniosAutonomos"/>
							<feature mandatory="true" name="FondosDeCapitalPrivado"/>
							<feature mandatory="true" name="CarterasColectivas"/>
						</or>
						<feature mandatory="true" name="Vehiculos"/>
					</and>
				</and>
				<and mandatory="true" name="GestionInmobiliaria">
					<and mandatory="true" name="GestionOperativa">
						<feature name="Equipamiento"/>
						<feature name="Instalaciones"/>
						<feature name="ActivosFisicos"/>
					</and>
					<feature name="ProcesosDeTasaciones"/>
					<feature name="ProcesosDeReservas"/>
					<feature name="Sistemas"/>
					<feature name="RecursosHumanos"/>
				</and>
			</and>
		</struct>
		<constraints>
			<rule>
				<eq>
					<imp>
						<var>Auditorias</var>
						<var>Valoraciones</var>
					</imp>
					<not>
						<var>RevisionesMercado</var>
					</not>
				</eq>
			</rule>
			<rule>
				<imp>
					<var>DefinicionGobiernoCorporativo</var>
					<not>
						<var>RelacionInversores</var>
					</not>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>Alternativas</var>
					<var>TipoTransferencia</var>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>Equipamiento</var>
					<not>
						<var>ActivosFisicos</var>
					</not>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>Instalaciones</var>
					<not>
						<var>ActivosFisicos</var>
					</not>
				</imp>
			</rule>
			<rule>
				<imp>
					<var>Contabilidad</var>
					<var>VentaActivos</var>
				</imp>
			</rule>
		</constraints>
		<calculations Auto="true" Constraints="true" Features="true" Redundant="true" Tautology="true"/>
		<comments/>
		<featureOrder userDefined="false"/>
	</featureModel>
